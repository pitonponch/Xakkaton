# -*- coding: utf-8 -*-
import telebot
from telebot import types

bot = telebot.TeleBot('5002670071:AAE9j6ruQYg-9QZZPtxSc0i7YFVhSU4NLMg')
@bot.message_handler(commands=['start'])
def start(message):
    bot.send_message(message.chat.id, 'Привет')

bot.polling()
def main():
    markup = types.ReplyKeyboardMarkup(True)
    key1 = types.KeyboardButton('Верхняя неделя')
    key2 = types.KeyboardButton('Нижняя неделя')
    markup.add(key1)
    markup.add(key2)
    return markup
